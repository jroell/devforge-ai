    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cron-parser": "^5.5.0",
    "diff": "^8.0.3",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.8.3",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.575.0",
    "mermaid": "^11.12.3",
    "openai": "^6.22.0",
    "prettier": "^3.8.1",
    "radix-ui": "^1.4.3",
